<form method="POST" action="/todos" class="form-inline">
  <input
    id="to-do-input"
    class="form-control mr-sm-2"
    type="text"
    name="name"
    placeholder="Add To-Do"
    aria-label="Search"
  />
  <button class="btn btn-outline-success my-2 my-sm-0" type="submit">
    Add
  </button>
</form>
